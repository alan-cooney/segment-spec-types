{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://eventfan.io/schemas/source/segment/b2b-saas/signed-up",
  "title": "Signed Up",
  "description": "User signs up for your service",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "description": "The username of the user"
    },
    "context": {
      "type": "array",
      "description": "The context array",
      "properties": {
        "group_id": {
          "type": "string",
          "description": "The id of the account being created/updated/deleted"
        }
      }
    },
    "signup_type": {
      "type": "string",
      "description": "The type of signup (viral or organic)"
    },
    "first_name": {
      "type": "string",
      "description": "User's first name"
    },
    "last_name": {
      "type": "string",
      "description": "User's last name"
    },
    "email": {
      "type": "string",
      "description": "User's email address"
    },
    "phone": {
      "type": "string",
      "description": "User's phone number"
    },
    "title": {
      "type": "string",
      "description": "User's salutation"
    }
  },
  "required": [
    "username",
    "email"
  ]
}