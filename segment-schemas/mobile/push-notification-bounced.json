{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://eventfan.io/schemas/source/segment/mobile/push-notification-bounced",
  "title": "Push Notification Bounced",
  "description": "Push notification from a provider bounces",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "description": "Custom string if notification is “actionable“ Default: “Open”"
    },
    "push_campaign": {
      "properties": {
        "source": {
          "type": "string",
          "description": "Push provider"
        },
        "name": {
          "type": "string",
          "description": "Campaign name"
        },
        "medium": {
          "type": "string",
          "description": "What type of link was pushed"
        },
        "content": {
          "type": "string",
          "description": "Content of the notification"
        }
      }
    }
  },
  "required": []
}