{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://eventfan.io/schemas/source/segment/mobile/install-attributed",
  "title": "Install Attributed",
  "description": "Segment or an integrated partner can discern the source of an install",
  "type": "object",
  "properties": {
    "provider": {
      "type": "string",
      "description": "Attribution provider"
    },
    "campaign": {
      "properties": {
        "source": {
          "type": "string",
          "description": "Campaign source -- attributed ad network"
        },
        "name": {
          "type": "string",
          "description": "Name of the attributed campaign"
        },
        "medium": {
          "type": "string",
          "description": "What type of link was used"
        },
        "content": {
          "type": "string",
          "description": "Content of the campaign"
        },
        "ad_creative": {
          "type": "string",
          "description": "Ad creative name"
        },
        "ad_group": {
          "type": "string",
          "description": "Ad group name"
        }
      }
    }
  },
  "required": []
}